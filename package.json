  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@types/elliptic": "6.4.12",
    "@types/express": "4.17.9",
    "@types/faker": "4.1.12",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/redis": "2.8.28",
    "@types/uuid": "8.3.0",
    "@vue/component-compiler-utils": "3.2.0",
    "cross-env": "7.0.2",
    "faker": "4.1.0",
    "jest": "26.6.3",
    "luxon": "1.25.0",
    "nodemon": "2.0.6",
    "parcel-bundler": "1.12.4",
    "pre-commit": "1.2.2",
    "prettier": "2.1.2",
    "pug": "3.0.0",
    "sass": "1.29.0",
    "ts-jest": "26.4.4",
    "typescript": "4.2.3",
    "vue-router": "3.4.9",
    "vue-template-compiler": "2.6.12"
  },
  "scripts": {
    "start": "node lib/server/server.js",
    "server": "nodemon lib/server/server.js",
    "server:peer": "cross-env START_AS_PEER='true' nodemon lib/server/server.js",
    "type-check": "tsc --noEmit",
    "ts:watch": "tsc -w",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "test:watch": "jest --watch",
    "test": "jest",
    "prettier": "prettier --write src/",
    "parcel:watch": "parcel watch src/frontend/index.html",
    "parcel:build": "parcel build src/frontend/index.html"
  },
  "pre-commit": [
    "prettier"
  ],
  "dependencies": {
    "body-parser": "1.19.0",
    "elliptic": "6.5.3",
    "express": "4.17.1",
    "got": "11.8.0",
    "hex-to-binary": "1.0.1",
    "redis": "3.0.2",
    "uuid": "8.3.2",
    "vue": "2.6.12",
    "vue-hot-reload-api": "2.3.4"
  }
}
